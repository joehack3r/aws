                "Protocol" : "1",
                "Icmp": {
                    "Type": "3",
                    "Code": "4"
                },
                "RuleAction" : "allow",
                "RuleNumber" : "120"
            }
        },

        "PublicInboundBastionHostNetworkAclEntry" : {
            "Type" : "AWS::EC2::NetworkAclEntry",
